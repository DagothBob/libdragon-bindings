{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "rust: cargo build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"type": "shell",
			"command": "cargo",
			"args": [
				"build",
				"--target", "mips-nintendo64-none.json", //"x86_64-unknown-linux-gnu"
				"-Z", "build-std=core,compiler_builtins,alloc",
				"-Z", "build-std-features=compiler-builtins-mem"
			],
			"problemMatcher": "$rustc"
		}
	]
}